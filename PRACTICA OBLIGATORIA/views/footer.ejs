<script language="javascript">
    function activar(elemento) {
        if (elemento.tipo.options[elemento.tipo.selectedIndex].text == "Sugerencia" || elemento.tipo.options[elemento.tipo.selectedIndex].text == "Incidencia") {
            document.getElementById("suginc").style.display = "inline";
            document.getElementById("felic").style.display = "none";
            document.getElementById("defecto").style.display = "none";
        } else {
            document.getElementById("felic").style.display = "inline";
            document.getElementById("suginc").style.display = "none";
            document.getElementById("defecto").style.display = "none";
        }
    }

    function habilitar(form) {
        if (form.tipo.options[0].selected || form.tipo.options[2].selected == true || form.tipo.options[3].selected == true) {
            form.ticTec.disabled = true;
            form.numEmpl.disabled = true;
        } else {
            form.ticTec.disabled = false;
            form.numEmpl.disabled = false;
        }
    }
</script>
